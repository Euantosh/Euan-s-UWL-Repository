atemptCount=0
while atemptCount < 5:
    print ("The Attempts left",(5-atemptCount))
    password=input("please Enter Password")
    atemptCount+=1
    if password!='letmein':
        print ("Acess denied")
    else:
        print("Welcome,(number of attempts",atemptCount,")")
        break

if atemptCount >= 5:
    print("You have Exceded the limit your locked out")
