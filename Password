atemptCount=0
password=input("please Enter Password")
if password!='letmein':
    print ("Acess denied")
    atemptCount+=1
    password=input(" please Enter Password"+"(second attempt)")
    if password!='letmein':
        print ("Acess denied")
        atemptCount+=1
        password=input(" please Enter Password"+"(Third attempt)")
        if password!='letmein':
            print ("Acess denied")
            atemptCount+=1
            password=input(" please Enter Password"+"(Fourth attempt)")
            if password!='letmein':
                print ("Acess denied")
                atemptCount+=1
                password=input(" please Enter Password"+"(Fith and final attempt)")
                atemptCount+=1
                if password!='letmein':
                    print ("Your locked out")
if password=='letmein':
    print ("password accepted,number of attempts",atemptCount)


####


while atemptCount < 5:

    password=input("please Enter Password")
    if password!='letmein':
        print ("Acess denied")
        atemptCount+=1
    else:
        print("Welcome")
        break

if atemptCount > 5:
    print("Exceded limit")


